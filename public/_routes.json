{
  "version": 1,
  "routes": [
    {
      "pattern": "/api/auth/login",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
      "script": "worker"
    },
    {
      "pattern": "/api/*",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
      "script": "worker"
    },
    {
      "pattern": "/*",
      "serve": "/index.html"
    }
  ]
}
